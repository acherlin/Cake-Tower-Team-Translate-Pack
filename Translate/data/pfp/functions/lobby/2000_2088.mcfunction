data merge block 31968 181 33476 {GlowingText: 1b, Color: "pink", Text4: '{"text":""}', x: 31968, Text3: '{"extra":[{"text":"---->"}],"text":""}', y: 181, Text2: '{"extra":[{"text":"治疗水晶"}],"text":""}', z: 33476, Text1: '{"extra":[{"text":"生成"}],"text":""}', id: "minecraft:sign"}
data merge block 31970 181 33475 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["你好，治疗师，欢迎来到治疗师训练。\n\n治疗师是团队的中流砥柱，他们的责任是确保每个人都活着。\n\n你可能已经注意到你的生命值很低，试着用这个医疗枪来治疗自己吧。"]}}, x: 31970, y: 181, Page: 0, z: 33475, id: "minecraft:lectern"}
data merge block 31974 181 33473 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["哈，看来医疗枪似乎哑火了？\n\n那是因为使用治疗物品需要消耗治疗水晶。\n\n治疗水晶是治疗物品的弹药，治疗师会在进入关卡时获得一些。"]}}, x: 31974, y: 181, Page: 0, z: 33473, id: "minecraft:lectern"}
data merge block 31958 181 33471 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["对治疗者而言，提高自身的治疗力属性很重要。\n\n治疗力越高，你在每个地牢中获得的治疗水晶就越多。\n\n治疗水晶是灵魂物品，意味着它们会在每个关卡结束后被清除。"]}}, x: 31958, y: 181, Page: 0, z: 33471, id: "minecraft:lectern"}
data merge block 31974 181 33464 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["治疗者可以看到一条特殊的 Boss 条，其上显示了所有玩家的生命值，并且，在治疗者的视野中，半血玩家会出现云粒子，而血量最低的玩家身上则会出现流血的粒子效果。\n\n这样可以帮助治疗者更好地分辨哪些玩家需要治疗"]}}, x: 31974, y: 181, Page: 0, z: 33464, id: "minecraft:lectern"}
