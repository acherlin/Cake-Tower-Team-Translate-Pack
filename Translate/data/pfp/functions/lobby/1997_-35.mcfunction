data merge block 32002 101 -473 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["斯旺\n骑士\n\n斯旺是最初发现高塔的人，他是一个热爱挑战的剑客，非常适合初学冒险家。"], display: {Name: '{"text":"斯旺"}'}}}, x: 32002, y: 101, Page: 0, z: -473, id: "minecraft:lectern"}
data merge block 32002 101 -468 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["小蒂米\n菜鸟\n\n小蒂米是捉迷藏时跑得离家有些远的小孩。\n\n他偶然发现并决定进入这座塔。", "小蒂米非常脆弱，也无法回复血量。\n\n他是游戏中最弱的角色，所以如果不是寻求挑战，建议不要玩他"]}}, x: 32002, y: 101, Page: 1, z: -468, id: "minecraft:lectern"}
data merge block 31998 101 -473 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["凯利\n肉盾\n\n凯利的职业是肉盾，会优先进行防御。但这并不意味着她很弱，事实上，做为一名骑士，她依然能够胜任战斗，装备着与她相伴的锤子为武器。\n\n她来到塔楼是为了找到她的", "真爱。不幸的是，她有点呆呆的，所以这个梦想看上去得花点功夫了……"], display: {Name: '{"text":"凯利"}'}}}, x: 31998, y: 101, Page: 0, z: -473, id: "minecraft:lectern"}
data merge block 31998 101 -468 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["乔伊\n吸血鬼\n\n乔伊是苏佩的异体。在和朋友们一起冲出白蛋糕实验室后，乔伊决定加入蛋糕小队一同登塔。他对血的渴望与日俱增。他的行为相当老练，但", "同时也非常喜欢开玩笑"]}}, x: 31998, y: 101, Page: 0, z: -468, id: "minecraft:lectern"}
data merge block 31998 101 -463 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["斗士 B'\n\n\nB'是本的异体\n\n\n亨利需要一个不在乎蜜蜂的本，这样在杀死本后就可以将本替换了。", "B'为自己的存在而愤怒，并为了寻求复仇而爬塔。\n\n他使用多种拳套来战斗，他十分强壮迅速但无法使用主动道具或是魔法。"]}}, x: 31998, y: 101, Page: 1, z: -463, id: "minecraft:lectern"}
data merge block 32027 101 -502 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["苏佩\n法师\n\n一名强大的法师，他利用自己对超级马里奥的热爱，从他们的世界中变出元素之花来对抗他的敌人。他表达能力很强，也常常爱开玩笑。"], display: {Name: '{"text":"苏佩"}'}}}, x: 32027, y: 101, Page: 0, z: -502, id: "minecraft:lectern"}
data merge block 32027 101 -498 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["坚果\n？？？\n坚果是凯利进入塔楼时背包里的一小块长着脸的粘稠物。\n\n塔楼给予了它生命，而现在它想却登上塔顶\n\n没人知道这是为什么……"], display: {Name: '{"text":"凯利"}'}}}, x: 32027, y: 101, Page: 0, z: -498, id: "minecraft:lectern"}
data merge block 32032 101 -498 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["暗黑斯旺\n黑暗术士\n\n暗黑斯旺是斯旺雷特的异体…他的灵魂替代品为黑暗徽章……\n\n与斯旺不同，暗黑斯旺的性格十分的悲观与急躁。他是黑暗魔法的大师。"]}}, x: 32032, y: 101, Page: 0, z: -498, id: "minecraft:lectern"}
data merge block 31998 101 -527 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["慕斯\n天使\n\n乐于炫耀自己在喜剧和医学领域成就的专家。他也许看上去弱不禁风，但他的治疗能力却是无与伦比的。他的羽翼以及保持漂浮的能力能使他免受伤害。"], display: {Name: '{"text":"慕斯"}'}}}, x: 31998, y: 101, Page: 0, z: -527, id: "minecraft:lectern"}
data merge block 31998 101 -532 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["果冻斯旺\n果冻人\n\n果冻斯旺是斯旺雷特的异体，他是由果冻一样的物质构成的。\n\n这使得他可以蹬墙跳，给予他很快的速度。", "他的治疗方式主要是随时间的缓慢再生。果冻斯旺即使从攻击中受到大量伤害也可以迅速恢复。\n\n然而斯旺令人讨厌的人格特质被他继承并放大了，所以没有人想和他说话。"], display: {Name: '{"text":"慕斯"}'}}}, x: 31998, y: 101, Page: 0, z: -532, id: "minecraft:lectern"}
data merge block 32002 101 -527 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["本\n养蜂人\n\n他是一名不大情愿养蜂的养蜂人，偶然间发现了这座塔。不知何故，他与这里的蜜蜂建立了深厚的关系，可以用它们的蜂蜜来提高队友的生命值。\n不过，似乎塔里正在酝酿着一场战争……"], display: {Name: '{"text":"慕斯"}'}}}, x: 32002, y: 101, Page: 0, z: -527, id: "minecraft:lectern"}
data merge block 31973 101 -498 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["雷琪\n弓箭手\n\n较平庸的神射手……或者说……神射“鸭”？至少来说，他精通射术，擅长在远距离击杀敌人，而他的宠物狗则负责解决那些近距离的敌人。\n\n", "他很自大，认为他几乎能轻松那拿下任何对手。他经常惨遭失败，因为他……不是最聪明的……"], display: {Name: '{"text":"雷琪"}'}}}, x: 31973, y: 101, Page: 0, z: -498, id: "minecraft:lectern"}
data merge block 31968 101 -498 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["JJ\n渔夫\n\nJJ是一位有抱负的渔民，他利用高塔的鱼群来帮助保持高血量。他总是看起来有个大计划，但从来没有进展。\nJJ用大钩子战斗，他还有一只值得信赖的宠物狗——超级杰伦。"]}}, x: 31968, y: 101, Page: 0, z: -498, id: "minecraft:lectern"}
data merge block 31973 101 -502 {Book: {id: "minecraft:writable_book", Count: 1b, tag: {pages: ["休\n快枪手\n\n休是一位退休的枪支专家，他把各种武器带到了塔里。\n\n他善于奔跑，也是位强大的战士……只要他能保持清醒。"], display: {Name: '{"text":"休"}'}}}, x: 31973, y: 101, Page: 0, z: -502, id: "minecraft:lectern"}



